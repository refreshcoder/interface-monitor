config settings 'settings'
    option enabled '1'
    list interfaces 'eth1'
    list interfaces 'eth2'
    list interfaces 'eth3'
    option monitor_interval '60'
    option connectivity_enable '0'
    option ping_interval '60'
    option connectivity_verbose '0'
    option db_enable '1'
    option db_path '/etc/interface_monitor.db'
    option db_retention_days '30'
